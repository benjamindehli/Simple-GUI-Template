    "babel-core": "^6.21.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.0",
    "gulp": "^3.9.1",
    "gulp-real-favicon": "^0.2.2",
    "gulp-rename": "^1.2.2",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.0",
