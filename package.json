{
  "name": "quark-gui",
  "version": "0.5.16",
  "description": "Webpack GUI framework based on atomic design",
  "keywords": [
    "atomic",
    "gui",
    "gui-template",
    "gui-toolkit",
    "webpack",
    "responsive",
    "front-end",
    "framework",
    "web",
    "js",
    "sass"
  ],
  "main": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "files": [
    "setup",
    "src/**/*.{ts,js,scss}",
    "src/fonts/**/*.{woff,woff2,eot,svg,ttf}",
    "lib/**/*.{js,d.ts}",
    "LICENSE"
  ],
  "scripts": {
    "test": "vows test/* --spec",
    "build": "rimraf dist && NODE_ENV=production webpack",
    "dev": "NODE_ENV=development node dev-server.js",
    "win-build": "rimraf dist && SET NODE_ENV=production&& webpack",
    "win-dev": "SET NODE_ENV=development&& node dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjamindehli/quarkGUI.git"
  },
